create table anniversaries(
       id INTEGER PRIMARY KEY AUTOINCREMENT,
       date TEXT UNIQUE,
       week_of_month INTEGER,
       day_of_week INTEGER,
       name VARCHAR(30) NOT NULL,
       description VARCHAR(30)
);

